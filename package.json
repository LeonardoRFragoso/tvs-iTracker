{
  "name": "tvs-digital-signage",
  "version": "1.0.0",
  "description": "Plataforma de TV Corporativa e Sinalização Digital",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm start\" \"cd backend && python app.py\"",
    "start:lan": "set HOST=0.0.0.0&& set PORT=3000&& set HTTPS=false&& set BROWSER=none&& react-scripts start",
    "start:tv": "set HOST=0.0.0.0&& set PORT=80&& set HTTPS=false&& set BROWSER=none&& react-scripts start",
    "build:tv": "set REACT_APP_API_URL=http://192.168.0.4/api&& npm run build",
    "dev:lan": "concurrently \"npm run start:lan\" \"cd backend && python app.py\"",
    "dev:tv": "concurrently \"npm run start:tv\" \"cd backend && set TV_MODE=true&& python app.py\"",
    "deploy:tv": "npm run build:tv && xcopy build backend\\build /e /i /y"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.14",
    "@fullcalendar/daygrid": "^6.1.14",
    "@fullcalendar/interaction": "^6.1.14",
    "@fullcalendar/react": "^6.1.14",
    "@fullcalendar/timegrid": "^6.1.14",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-calendar": "^4.6.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
